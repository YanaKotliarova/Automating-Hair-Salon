<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Client.Client">
   <children>
      <Label layoutX="30.0" layoutY="40.0" text="Добро пожаловать!">
         <font>
            <Font size="48.0" />
         </font>
      </Label>
      <Button layoutX="226.0" layoutY="224.0" mnemonicParsing="false" onAction="#buttomEnter" prefHeight="29.0" prefWidth="148.0" text="Войти">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button layoutX="226.0" layoutY="288.0" mnemonicParsing="false" onAction="#SignUp" text="Зарегестрироваться">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button layoutX="562.0" layoutY="14.0" mnemonicParsing="false" onAction="#CloseWindow" text="X" />
      <TextField fx:id="Login" layoutX="226.0" layoutY="134.0" prefHeight="29.0" prefWidth="148.0" promptText="Логин">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <PasswordField fx:id="Password" layoutX="226.0" layoutY="178.0" prefHeight="29.0" prefWidth="148.0" promptText="Пароль">
         <font>
            <Font size="14.0" />
         </font>
      </PasswordField>
   </children>
</AnchorPane>
