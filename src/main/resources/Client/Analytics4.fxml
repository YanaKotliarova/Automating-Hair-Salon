<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Client.Client">
   <children>
      <BarChart fx:id="barChart" layoutX="50.0">
        <xAxis>
          <CategoryAxis label="Ценовой сегмент" side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis label="Количество услуг" side="LEFT" />
        </yAxis>
      </BarChart>
      <Button layoutX="15.0" layoutY="14.0" mnemonicParsing="false" onAction="#analyticsCost" text="Добавить" />
   </children>
</AnchorPane>
